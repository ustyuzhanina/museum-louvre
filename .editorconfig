# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
trim_trailing_whitespace = true
insert_final_newline = true
tab_width = 2

[*.ts]
quote_type = single

[*.md]
max_line_length = off
trim_trailing_whitespace = false

